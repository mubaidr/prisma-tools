  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@paljs/create": "^4.0.12",
    "@paljs/display": "^4.0.0",
    "@paljs/generator": "^4.0.12",
    "@paljs/schema": "^4.0.6",
    "@prisma/sdk": "3.0.1",
    "cross-spawn": "^7.0.3",
    "enquirer": "^2.3.6",
    "fs-extra": "^10.0.0",
    "pkg-dir": "^5.0.0",
    "prettier": "^2.3.2",
    "tslib": "^2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.12",
    "@types/mocha": "^9",
    "@types/node": "16.7.13",
    "chai": "^4",
    "eslint": "^7.32",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.2",
    "globby": "^11",
    "mocha": "^9",
    "nyc": "^15",
    "ts-node": "^10",
    "typescript": "4.4.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://paljs.com",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "pal",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "paljs/prisma-tools",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "e6a586cf31d1f4312a008655d5566fad5b4549de"
